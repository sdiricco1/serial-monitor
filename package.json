{
  "name": "serial-monitor",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.6.3",
    "@electron/remote": "^1.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "antd": "^4.16.11",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "serialport": "^9.2.0"
  },
  "productName": "Serial monitor",
  "description": "A simple serial monitor",
  "main": "public/electron_main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/electron_main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "electron-rebuild": ".\\node_modules\\.bin\\electron-rebuild.cmd",
    "electron-rebuild-serialport": "electron-rebuild -f -w serialport"
  },
  "build": {
    "extends": null,
    "appId": "com.example.electron-cra",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "author": "",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^3.1.1",
    "prettier": "^2.2.1",
    "wait-on": "^5.2.1"
  }
}
